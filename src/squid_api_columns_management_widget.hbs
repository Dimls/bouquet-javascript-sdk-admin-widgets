<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
  <h4 class="modal-title" id="myModalLabel">{{typeLabelPlural}}</h4>
</div>
<div class="modal-body">
    <div class="squid-api-collection-management-widget">
        <div class="squid-api-admin-widget-columns-management">
            <select multiple="multiple">
                {{#each dynamic}}
                    <option {{#if parentId}} class="child" {{/if}} value="{{id}}">{{name}}</option>
                {{/each}}
                {{#each nonDynamic}}
                    <option value="{{id}}" {{#if parentId}} class="child{{depth}}" {{/if}} selected="selected">{{name}}</option>
                {{/each}}
            </select>
            <div class="management">
                <button type="button" class="btn btn-default create">
                    Create
                </button>
                <button type="button" class="btn btn-default edit" disabled="true">
                    Edit
                </button>
                <button type="button" class="btn btn-default delete" disabled="true">
                    Delete
                </button>
                <button type="button" class="btn btn-default close-modal" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</div>
<!--  end of modal - -->
</div>
